#docker build -f Dockerfiletest . --no-cache --build-arg CURL_VERSION=1 --build-arg QUICHE_VERSION=2

FROM debian:12-slim

ENV CURL_VERSION 7567
ENV QUICHE_VERSION 65656

ARG CURL_VERSION
ARG QUICHE_VERSION

ENV CURL_VERSION $CURL_VERSION
ENV QUICHE_VERSION $QUICHE_VERSION

RUN echo $CURL_VERSION  $QUICHE_VERSION
RUN echo skobskob ${CURL_VERSION}  ${QUICHE_VERSION}

ENV CURL_VERSION   $CURL_VERSION
ENV QUICHE_VERSION $QUICHE_VERSION

RUN echo $CURL_VERSION  $QUICHE_VERSION
RUN echo skobskob ${CURL_VERSION}  ${QUICHE_VERSION}